class Solution {
public:
    int numJewelsInStones(string jewels, string stones) {
        int count=0;
        for(int i=0;i<jewels.size();i++)
        {
            char ch=jewels[i];
            for(int i=0;i<stones.size();i++)
            {
                if(ch==stones[i])  count++;

            }
        }
        return count;
    }
};

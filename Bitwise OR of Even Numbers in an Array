class Solution {
public:
    int evenNumberBitwiseORs(vector<int>& nums) 
    {
      int bitwiseor=0;
        for(int i=0;i<nums.size();i++)
            {
                if(nums[i]%2==0)
                {    
                bitwiseor=bitwiseor | nums[i];
                }    
            }    
       return bitwiseor;
    }
};

class Solution {
public:
    int countSegments(string s) {
        vector<string> temp;
        for(int i=0;i<s.size();i++)
        {
            if(s[i]!=' ')
            {
                string ch;
                while(i < s.size() && s[i]!=' ')
                {
                   ch+=s[i];
                   i++;
                }
                temp.push_back(ch);
            }
        }
        return temp.size();
        
    }
};

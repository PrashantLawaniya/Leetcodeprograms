class Solution {
public:
    vector<int> replaceElements(vector<int>& arr) {
        vector<int> result;
        if(arr.size()==1)  return {-1};
        for(int i=0;i<arr.size()-1;i++)
        {
            int maxi=arr[i+1];
            for(int j=i+1;j<arr.size();j++)
            {
               maxi=max(maxi,arr[j]);
            }
          
            result.push_back(maxi);
           

        }
         result.push_back(-1);
        return result;
    }
};

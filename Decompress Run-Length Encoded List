class Solution {
public:
    vector<int> decompressRLElist(vector<int>& nums) {
        int i=0;
        int j=1;
        vector <int> ans;
        while(j<nums.size())
        {
            while(nums[i]!=0)
            {
                ans.push_back(nums[j]);
                nums[i]--;
            }
            i+=2;
            j+=2;
        }
        return ans;
    }
};

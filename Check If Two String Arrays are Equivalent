class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string ans1="";
       
         for(auto p:word1)   ans1+=p;
        
        string ans2="";
       
          for(auto p:word2)  ans2+=p;
        
        if(ans1==ans2)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
};

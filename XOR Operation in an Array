class Solution {
public:
    int xorOperation(int n, int start) {
        vector<int> ans(n,0);
        for(int i=0;i<n;i++)
        {
            ans[i]=start+2*i;
        }
        int XOR=0;
        for(int i=0;i<ans.size();i++)
        {
            XOR=XOR^ans[i];
        }
        return XOR;
    }
};

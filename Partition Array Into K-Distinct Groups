class Solution {
public:
    bool partitionArray(vector<int>& nums, int k) {
        unordered_map<int,int> mpp;
        for(int var:nums)
        {
            mpp[var]++;
        }
        for(auto pair:mpp)
        {
            if(pair.second>nums.size()/k)  return false;
        }
         return nums.size()%k==0;
    }
};
